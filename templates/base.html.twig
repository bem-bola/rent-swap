<!DOCTYPE html>
<html lang="fr">
    <head>
        <script type="text/javascript" src="{{ asset('build/tarteaucitron/tarteaucitron.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/tarteaucitron/js/tarteaucitron-init.js') }}">

        </script>

        <meta charset="UTF-8">
        <meta name="description" content="{% block description %}{% endblock %}">
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('style') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
        {% block head %}
        {% endblock %}
        <link rel="icon" href="{{ asset('img/logo/logo-reusiix.png') }}" type="image/svg">
        <script src="{{ asset('bundles/fosjsrouting/js/router.min.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
    </head>
    <body class="bg-theme-gray-3">
        <!-- Header-->
        {% block header %}
            {% include '_partial/header.html.twig' %}
        {% endblock %}
        <main class="px-xs-3 px-sm-3 px-md-3 px-lg-5 mb-5">
            {% include '_partial/alert.html.twig' %}
            {% block main %}{% endblock %}
        </main>
        <!-- Footer-->
        {% block footer %}
            {% include '_partial/footer.html.twig' %}
        {% endblock %}
    </body>

</html>
